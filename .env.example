# Copy this file to functions/.env.local for local development
# For production, use Firebase Functions config: firebase functions:config:set

# OpenAI API Key (for AI marketing copy generation)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic API Key (alternative for AI generation)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# SendGrid API Key (for email notifications - optional)
SENDGRID_API_KEY=SG.your-sendgrid-key-here

# Slack Webhook (for notifications - optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Custom Configuration
MAX_FILE_UPLOAD_SIZE_MB=50
DEFAULT_PAGINATION_LIMIT=20
ENABLE_DEBUG_LOGS=true

# CORS Allowed Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com,https://app.yourdomain.com

# Timezone for scheduled functions
TIMEZONE=America/New_York

